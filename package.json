{
  "name": "@scayle/nuxt-opentelemetry",
  "version": "0.6.2",
  "description": "A Nuxt module for OpenTelemetry integration",
  "author": "SCAYLE Commerce Engine",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "opentelemetry"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "url": "git+https://github.com/scayle/nuxt-opentelemetry.git"
  },
  "website": "https://scayle.dev/en/storefront-guide/developer-guide/integrations/open-telemetry",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "type": "module",
  "types": "./dist/module.d.ts",
  "files": [
    "CHANGELOG.md",
    "dist/**"
  ],
  "engines": {
    "node": ">= 18.15.0"
  },
  "compatibility": {
    "nuxt": "^3.10"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prep": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "format": "dprint check",
    "format:fix": "dprint fmt",
    "lint": "eslint . --format gitlab",
    "lint:fix": "eslint . --fix",
    "package:lint": "publint",
    "typecheck": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.3",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.56.1",
    "@opentelemetry/exporter-trace-otlp-proto": "0.57.2",
    "@opentelemetry/instrumentation": "0.57.2",
    "@opentelemetry/resources": "1.30.1",
    "@opentelemetry/sdk-node": "0.52.1",
    "@opentelemetry/semantic-conventions": "1.30.0",
    "@vercel/otel": "1.10.2",
    "defu": "^6.1.4",
    "knitwork": "^1.1.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.2.0",
    "@nuxt/eslint-config": "1.1.0",
    "@nuxt/module-builder": "0.8.4",
    "@nuxt/schema": "3.15.4",
    "@scayle/eslint-config-storefront": "4.4.1",
    "@types/node": "22.13.10",
    "dprint": "0.49.0",
    "eslint": "9.22.0",
    "eslint-formatter-gitlab": "5.1.0",
    "nuxt": "3.15.4",
    "publint": "0.2.12",
    "rimraf": "6.0.1",
    "typescript": "5.8.2",
    "vue-tsc": "2.2.8"
  },
  "volta": {
    "node": "22.14.0"
  }
}
